<% if @users.any? %>
  $('[data-is-list="users"]').html("<%= escape_javascript(render partial: 'user/events/searched_invited_users/invitation_list', locals: { users: @users } ) %>");
<% elsif !@no_search%>
  $('[data-is-list="users"]').html("<%= escape_javascript(render partial: 'user/events/searched_invited_users/no_result') %>");
<% else %>
  $('[data-is-list="users"]').html("");
<% end %>